{
   "version": 1,
   "disable_existing_loggers": true,
   "formatters": {
      "standard": {
         "format": "%(asctime)s:%(levelname)s:%(name)s:%(funcName)s:%(lineno)d:%(message)s"
      }
   },
   "handlers": {
      "default": {
         "level": "INFO",
         "formatter": "standard",
         "class": "logging.StreamHandler",
         "stream": "ext://sys.stdout"
      },
      "file": {
         "level": "INFO",
         "formatter": "standard",
         "class": "logging.handlers.RotatingFileHandler",
         "filename": "/tmp/sample.log",
         "mode": "a",
         "maxBytes": 1000000,
         "backupCount": 5
      }
   },
   "loggers": {
      "": {
         "handlers": [
            "default"
         ],
         "level": "WARNING",
         "propagate": false
      },
      "api.api": {
         "handlers": [
            "file"
         ],
         "level": "INFO",
         "propagate": false
      },
      "inf.initialize": {
         "handlers": [
            "file"
         ],
         "level": "INFO",
         "propagate": false
      },
      "evo.neuroembryogenesis": {
         "handlers": [
            "file"
         ],
         "level": "INFO",
         "propagate": false
      },
      "evo.connectome": {
         "handlers": [
            "file"
         ],
         "level": "INFO",
         "propagate": false
      },
      "evo.genome": {
         "handlers": [
            "file"
         ],
         "level": "INFO",
         "propagate": false
      },
      "__main__": {
         "handlers": [
            "default"
         ],
         "level": "DEBUG",
         "propagate": false
      }
   }
}